{
  "name": "dlog-nodejs",
  "version": "0.1.3",
  "description": "Pure Node.JS bindings for dlog",
  "main": "lib/index.js",
  "author": "lennartvrg",
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.3",
    "neon-cli": "^0.8.0"
  },
  "scripts": {
    "postinstall": "node-pre-gyp install --fallback-to-build=false"
  },
  "homepage": "https://github.com/lennartvrg/dlog",
  "repository": {
    "type": "git",
    "url": "https://github.com/lennartvrg/dlog.git",
    "directory": "nodejs"
  },
  "files": [
    "lib"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://dlog-native.s3.eu-central-1.amazonaws.com",
    "remote_path": "{version}",
    "package_name": "native-{platform}-{arch}.tar.gz",
    "module_path": "./native/lib",
    "pkg_path": "."
  }
}
