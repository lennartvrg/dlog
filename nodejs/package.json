{
  "name": "dlog-nodejs",
  "version": "0.0.6",
  "description": "Pure Node.JS bindings for dlog",
  "main": "lib/index.js",
  "author": "lennartvrg",
  "license": "MIT",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.3",
    "neon-cli": "^0.8.0"
  },
  "devDependencies": {
    "node-pre-gyp-github": "^1.4.3"
  },
  "scripts": {
    "build": "cd native && cargo build",
    "build-release": "cd native && cargo build --release",
    "install": "node-pre-gyp install --fallback-to-build=false || npm run build-release",
    "upload-binary": "npm run build-release && node-pre-gyp package && node-pre-gyp-github publish"
  },
  "homepage": "https://github.com/lennartvrg/dlog",
  "repository": {
    "type": "git",
    "url": "https://github.com/lennartvrg/dlog.git",
    "directory": "nodejs"
  },
  "files": [
    "lib"
  ],
  "binary": {
    "module_name": "index",
    "host": "https://github.com/lennartvrg/dlog/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native/build",
    "pkg_path": "."
  }
}
